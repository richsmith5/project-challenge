<%= simple_form_for @dog do |f| %>
  <div>
      <%= f.input :name %>
      <%= f.input :description, as: :text %>
  </div>

  <div>
      <%= f.label :images, 'image' %>
      <%= f.file_field :images, {multiple: true} %>
  </div>

  <% if @dog.images.any? %>
      <p>PUPPY PICS</p>
      <% @dog.images.each do |image| %>
      <%= image_tag image, class: 'uploaded_image' %> <br>
      <% end %>
  <% end %>

  <div>
      <%= f.submit  'Submit' %>
  </div>
<% end %>